{
    "processing_params": {
        "files": {
            "original_file": {
                "file_path": "../data/en.openfoodfacts.org.products.csv",
                "delimiter": "\t",
                "nrows": "10000",
                "col_exclusion": []
            },
            "processed_file": {
                "file_path": "../data/en.openfoodfacts.org.products_processed.csv",
                "delimiter": ","
            }
        },
        "actions": {
            "drop": {
                "drop_missing_energy": {
                    "mode": "missing",
                    "cols": [
                        "energy-kj_100g",
                        "energy-kcal_100g",
                        "energy_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_fat": {
                    "mode": "missing",
                    "cols": [
                        "fat_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_saturated_fat": {
                    "mode": "missing",
                    "cols": [
                        "saturated-fat_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_carbohydrates": {
                    "mode": "missing",
                    "cols": [
                        "carbohydrates_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_sugars": {
                    "mode": "missing",
                    "cols": [
                        "sugars_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_proteins": {
                    "mode": "missing",
                    "cols": [
                        "proteins_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_missing_salt": {
                    "mode": "missing",
                    "cols": [
                        "salt_100g"
                    ],
                    "approx_thresh": 1
                },
                "drop_abnormal_mandatory": {
                    "mode": "abnormal",
                    "boundaries": [
                        0,
                        100
                    ],
                    "cols": [
                        "fat_100g",
                        "saturated-fat_100g",
                        "carbohydrates_100g",
                        "sugars_100g",
                        "proteins_100g",
                        "salt_100g"
                    ]
                },
                "drop_format": {
                    "mode": "format",
                    "cols": "code",
                    "format_size": "13",
                    "format_char": "0"
                }
            }
        },
        "SQL": {
            "createdb_script_path": "../database/create_db.sql",
            "create_table_script_path": "../database/create_table.sql",
            "seed_script_path": "../database/seed.sql",
            "db_name": "open_food_data_db",
            "table_name": "records"
        }
    },
    "DB_params": {
        "credentials": {
            "host": "localhost",
            "database": "open_food_data_db",
            "user": "postgres",
            "password": ""
        }
    }
}